Cell["Step - by - Step Voting - Dev", "Subsection",
 CellChangeTimes->{{3.732300605498399*^9, 3.732300618700204*^9}, {
  3.732485077857809*^9, 3.73248508223216*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"FCMJoinByVote", "[", 
   RowBox[{
    RowBox[{"nodespec_", "?", "MatrixQ"}], ",", "fcms_List", ",", 
    RowBox[{"wgts_:", "0"}], ",", 
    RowBox[{"sz_Real:", "0.25"}], ",", " ", 
    RowBox[{"asz_Real:", "0.02"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", "\n", "\t", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"n", "=", 
       RowBox[{"Length", "@", "fcms"}]}], ",", "ws", ",", "fincm", ",", 
      "newedgs", ",", "ucands", ",", 
      RowBox[{"candedgs", "=", 
       RowBox[{"(", 
        RowBox[{"EdgeList", "/@", "fcms"}], ")"}]}], ",", "votes", ",", 
      "vetoedLnks"}], "}"}], ",", "\n", "\t", 
    RowBox[{
     RowBox[{"ws", " ", "=", " ", 
      RowBox[{"If", "[", "\n", "\t\t", 
       RowBox[{
        RowBox[{"n", "==", 
         RowBox[{"Length", "@", "wgts"}]}], ",", "\n", "\t\t", 
        RowBox[{"(", 
         RowBox[{"wgts", "/", 
          RowBox[{"Total", "[", "wgts", "]"}]}], ")"}], ",", "\n", "\t\t", 
        RowBox[{"ConstantArray", "[", 
         RowBox[{
          RowBox[{"1", "/", "n"}], ",", "n"}], "]"}]}], "\n", "\t", "]"}]}], 
     ";", "\n", "\t", 
     RowBox[{"ucands", " ", "=", " ", 
      RowBox[{"Sort", "@", 
       RowBox[{"Union", "@", 
        RowBox[{"Flatten", "@", "candedgs"}]}]}]}], ";", "\n", "\t", 
     RowBox[{"votes", "=", 
      RowBox[{"Outer", "[", "\n", "\t\t", 
       RowBox[{
        RowBox[{
         RowBox[{"Boole", "@", 
          RowBox[{"Not", "@", 
           RowBox[{"FreeQ", "[", 
            RowBox[{
             RowBox[{"candedgs", "[", 
              RowBox[{"[", "#2", "]"}], "]"}], ",", "#1"}], "]"}]}]}], "&"}], 
        ",", "\n", "\t\t", "ucands", ",", " ", "\n", "\t\t", 
        RowBox[{"Range", "@", 
         RowBox[{"Length", "@", "candedgs"}]}]}], "\n", "\t\t", "]"}]}], ";", 
     " ", 
     RowBox[{"(*", 
      RowBox[{"candedgs", " ", "is", " ", "__List"}], "*)"}], "\n", "\t", 
     RowBox[{"vetoedLnks", " ", "=", " ", 
      RowBox[{"ucands", "[", 
       RowBox[{"[", 
        RowBox[{"Select", "[", 
         RowBox[{
          RowBox[{"Range", "@", 
           RowBox[{"Length", "@", "ucands"}]}], ",", " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"Total", "[", 
               RowBox[{"Transpose", "@", "votes"}], "]"}], "[", 
              RowBox[{"[", "#", "]"}], "]"}], "<", 
             RowBox[{"Ceiling", "[", 
              RowBox[{"n", "/", "2"}], "]"}]}], ")"}], "&"}]}], "]"}], "]"}], 
       "]"}]}], ";", "\n", "\t", 
     RowBox[{"fincm", " ", "=", " ", 
      RowBox[{"Table", "[", "\n", "\t\t", 
       RowBox[{
        RowBox[{"EdgeDelete", "[", " ", 
         RowBox[{"f", ",", 
          RowBox[{"Intersection", "[", 
           RowBox[{
            RowBox[{"EdgeList", "[", "f", "]"}], ",", " ", "vetoedLnks"}], 
           "]"}]}], " ", "]"}], ",", " ", "\n", "\t\t", 
        RowBox[{"{", 
         RowBox[{"f", ",", "fcms"}], "}"}]}], "\n", "\t", "]"}]}], ";", "\n", 
     "\t", 
     RowBox[{"fincm", " ", "=", " ", 
      RowBox[{"FCMJoin", "[", 
       RowBox[{"nodespec", ",", "fincm", ",", "ws"}], "]"}]}], ";", "\n", 
     "\t", 
     RowBox[{"Return", "[", "fincm", "]"}], ";"}]}], "\n", "]"}]}]], "Input"],

